x_array = np.fromfile('%s/x.dat' % sol_dir, dtype='f8')
nx = len(x_array)
sol = np.fromfile('%s/sol.dat' % sol_dir, dtype='f8').reshape((112, nx))
f_l = energy(67)
NX = len(f_l)
F_l = np.zeros(NX)
for k in range(0,NX):
 F_l[k] = k * f_l[k]
plt.plot(F_l[0:NX])

plt.legend([r'$\alpha_{\phi}=0.05$', r'$\alpha_{\phi}=0.07$', r'$\alpha_{\phi}=0.1$'], loc='upper right')
plt.rcParams["figure.figsize"] = fig_size
plt.xscale('log')
plt.axhline(0,color='black',linewidth=0.6)
plt.axis([1,10000,0,0.28])
plt.title(r'Asymmetric Gaussian-1, $m_{\phi}t_{*}=14.00$, $m_{\phi}t_{p}=128$, $m_{\phi}t=130$', fontsize=17, fontweight='bold')
plt.ylabel(r'$(m_{\phi}x_p/{\pi})k\cdot\epsilon (k)$', fontsize=14)
plt.xlabel(r'$(m_{\phi}x_p/{\pi})k$', fontsize=14)
plt.savefig('Asymmetric_Gaussian_1_ts=14_tp=128_t=130_logscale_2.png')
plt.close()
