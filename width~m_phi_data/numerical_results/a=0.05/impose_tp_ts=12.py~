def energy(s):
    delta_t = t_array[s-1] - t_array[s+1]
    spc = spectrum(s)
    sps = sine(s)
    sps[0] = 0
    spsa = sine(s+1)
    spsb = sine(s-1)
    sp = spectrum(s)
    sp[0] = sp[0] / (2)**0.5
    spb = spectrum(s-1)
    spb[0] = sp[0] / (2)**0.5
    spa = spectrum(s+1)
    spa[0] = sp[0] / (2)**0.5
    L = len(sp)
    def spectrum_dot(s):
        sD = np.zeros(L)
        for j in range(1,L):
         sD[j] = (spa[j] - spb[j]) / delta_t
        return sD
    def s_spectrum_dot(s):
        sD = np.zeros(L)
        for j in range(1,L):
         sD[j] = (spsa[j] - spsb[j]) / delta_t
        return sD
    sd = spectrum_dot(s)
    sds = s_spectrum_dot(s)
    E = np.zeros(L)
    for i in range(0,L):
     E[i] = (sd[i]*sd[i]/4) + (sp[i]*sp[i]*(i*math.pi/256)*(i*math.pi/256)/4) + (sds[i]*sds[i]/4) + (sps[i]*sps[i]*(i*math.pi/256)*(i*math.pi/256)/4) #+ V(sp[i])/2 +  V(sps[i])/2 + (0.05 / L) - (0.05 * spc[0] / L) # - (1/spc[0]) * V(spc[i]) 
    return E

