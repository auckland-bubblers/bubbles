for k in range(0,NX):
 F_l[k] = k * f_l[k]
plt.plot(F_l[0:NX])

data2 = np.genfromtxt("a=0.05_ts=17_tp=362.txt", skip_header=3)
line = plt.plot(data2[:,0], 0.5 * data2[:,1])
#plt.setp(line, linestyle='--')
plt.setp(line, linewidth=2)


plt.legend([r'Cos.Sq.-Gaussian-2($m_{\phi}t_{p}=181$, $m_{\phi}t=184$)', r'Analytic($m_{\phi}t_{p}=181$, $t=\sqrt{t_{*}^{2}+x_{p}^{2}}$)', r'Cos.Sq.-Gaussian-2($m_{\phi}t_{p}=362$, $m_{\phi}t=368$)', r'Analytic($m_{\phi}t_{p}=362$, $t=\sqrt{t_{*}^{2}+x_{p}^{2}}$)'], loc='upper right')
plt.rcParams["figure.figsize"] = fig_size
plt.xscale('log')
plt.axhline(0,color='black',linewidth=0.6)
plt.axis([1,10000,0,0.20])
plt.title(r'Cos.Sq.-Gaussian-2, $\alpha_{\phi}=0.05$, $m_{\phi}t_{*}=14.14$', fontsize=17, fontweight='bold')
plt.ylabel(r'$(m_{\phi}x_p/{\pi})k\cdot\epsilon (k)$', fontsize=14)
plt.xlabel(r'$(m_{\phi}x_p/{\pi})k$', fontsize=14)
plt.savefig('Cos.Sq._Gaussian_2_a=0.05_t*=16.97_logscale_2.png')
plt.close()


def energy(s):
    delta_t = t_array[s-1] - t_array[s+1]
    spc = spectrum(s)
    sps = sine(s)
    sps[0] = 0
    spsa = sine(s+1)
    spsb = sine(s-1)
    sp = spectrum(s)
    sp[0] = sp[0] / (2)**0.5
    spb = spectrum(s-1)
    spb[0] = sp[0] / (2)**0.5
    spa = spectrum(s+1)
    spa[0] = sp[0] / (2)**0.5
    L = len(sp)
    def spectrum_dot(s):
        sD = np.zeros(L)
        for j in range(1,L):
         sD[j] = (spa[j] - spb[j]) / delta_t
        return sD
    def s_spectrum_dot(s):
        sD = np.zeros(L)
        for j in range(1,L):
         sD[j] = (spsa[j] - spsb[j]) / delta_t
        return sD
    sd = spectrum_dot(s)
    sds = s_spectrum_dot(s)
    E = np.zeros(L)
    for i in range(0,L):
     E[i] = (sd[i]*sd[i]/4) + (sp[i]*sp[i]*(i*math.pi/128)*(i*math.pi/128)/4) + (sds[i]*sds[i]/4) + (sps[i]*sps[i]*(i*math.pi/128)*(i*math.pi/128)/4) #+ V(sp[i])/2 +  V(sps[i])/2 + (0.05 / L) - (0.05 * spc[0] / L) # - (1/spc[0]) * V(spc[i]) 
    return E

