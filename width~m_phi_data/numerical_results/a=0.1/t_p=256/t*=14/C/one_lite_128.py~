    F_l = np.zeros(NX)
    for k in range(0,NX):
     F_l[k] = k * f_l[k]
    plt.plot(F_l[0:NX])

plt.axhline(0,color='black',linewidth=0.6)
plt.axis([1,5000,0,0.10])
plt.title(r'tanh, $\alpha_{\phi}=0.05$, $t_{p}/m_{\phi}=128$, $t_{*}/m_{\phi}=4.70$', fontsize=14.1, fontweight='bold')
plt.ylabel(r'$(x_p/{\pi})k\cdot\epsilon (k)$', fontsize=14)
plt.xlabel(r'$(x_p/{\pi})k$', fontsize=14)
plt.xscale('log')
plt.legend(['t=100', 't=110', 't=120', 't=130', 't=140', 't=150'], loc='upper right')
plt.savefig('tanh_100-150_logscale_2.png')

plt.close()

num_plots = 6

colormap = plt.cm.gist_rainbow
plt.gca().set_color_cycle([colormap(i) for i in np.linspace(0, 0.9, num_plots)])

for i in range(0,num_plots):
    f_l = energy(49 + (12 * i))
    NX = len(f_l)
    F_l = np.zeros(NX)
    for k in range(0,NX):
     F_l[k] = k * f_l[k]
    plt.plot(F_l[0:NX])

plt.axhline(0,color='black',linewidth=0.6)
plt.axis([1,5000,0,0.10])
plt.title(r'tanh, $\alpha_{\phi}=0.05$, $t_{p}/m_{\phi}=128$, $t_{*}/m_{\phi}=4.70$', fontsize=14.1, fontweight='bold')
plt.ylabel(r'$(x_p/{\pi})k\cdot\epsilon (k)$', fontsize=14)
plt.xlabel(r'$(x_p/{\pi})k$', fontsize=14)
plt.xscale('log')
plt.legend(['t=100', 't=120', 't=140', 't=160', 't=180', 't=190', 't=200'], loc='upper right')
plt.savefig('tanh_100-200_logscale_2.png')

plt.close()
