time_steps = len(t_array)
x_array = np.fromfile('%s/x.dat' % sol_dir, dtype='f8')
nx = len(x_array)
sol = np.fromfile('%s/sol.dat' % sol_dir, dtype='f8').reshape((121, nx))
f_l = energy(67)
NX = len(f_l)
F_l = np.zeros(NX)
for k in range(0,NX):
 F_l[k] = k * f_l[k]
plt.plot(F_l[0:NX])

data1 = np.genfromtxt("a=0.05_ts=20_tp=362.txt", skip_header=3)
line = plt.plot(data1[:,0], data1[:,1])
#plt.setp(line, linestyle='--')
plt.setp(line, linewidth=2)

data2 = np.genfromtxt("a=0.07_ts=20_tp=362.txt", skip_header=3)
line = plt.plot(data2[:,0], data2[:,1])
#plt.setp(line, linestyle='--')
plt.setp(line, linewidth=2)

data3 = np.genfromtxt("a=0.1_ts=20_tp=362.txt", skip_header=3)
line = plt.plot(data3[:,0], data3[:,1])
#plt.setp(line, linestyle='--')
plt.setp(line, linewidth=2)

plt.legend([r'$\alpha_{\phi}=0.05$', r'$\alpha_{\phi}=0.07$', r'$\alpha_{\phi}=0.1$', r'Analytic($\alpha_{\phi}=0.05$, $t=\sqrt{t_{*}^{2}+x_{p}^{2}}$)', r'Analytic($\alpha_{\phi}=0.07$, $t=\sqrt{t_{*}^{2}+x_{p}^{2}}$)', r'Analytic($\alpha_{\phi}=0.1$, $t=\sqrt{t_{*}^{2}+x_{p}^{2}}$)'], loc='upper right')
plt.rcParams["figure.figsize"] = fig_size
plt.xscale('log')
plt.axhline(0,color='black',linewidth=0.6)
plt.axis([1,10000,0,0.20])
plt.title(r'Cosine-Gaussian-2, $m_{\phi}t_{*}=19.80$, $m_{\phi}t_{p}=362$, $m_{\phi}t=368$', fontsize=17, fontweight='bold')
plt.ylabel(r'$(m_{\phi}x_p/{\pi})k\cdot\epsilon (k)$', fontsize=14)
plt.xlabel(r'$(m_{\phi}x_p/{\pi})k$', fontsize=14)
